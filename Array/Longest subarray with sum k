class Solution{
    int longSubarrWthSumDivByK(int arr[], int N, int k)
    {
        // Complete the function
       int maxl = 0;
       for (int i = 0; i < N; i++) {
            int sum1 = 0;
            for (int j = i; j < N; j++) {
               sum1 += arr[j];
               if (sum1 % k == 0)
                   maxl = Math.max(maxl, j - i + 1);
            }
         }
 
        return maxl;
    }
 
}
